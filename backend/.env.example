# Token de acesso do Dropbox para integração com planilha
DROPBOX_ACCESS_TOKEN=SEU_TOKEN_DROPBOX_AQUI
# Configuração do Neon DB (produção)
# Obtenha sua connection string no dashboard do Neon
DATABASE_URL=postgresql://username:password@ep-hostname.region.neon.tech/dbname?sslmode=require
POSTGRES_URL=postgresql://username:password@ep-hostname.region.neon.tech/dbname?sslmode=require

# Configurações locais (desenvolvimento)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=requisicao_db
DB_USER=postgres
DB_PASSWORD=sua_senha_aqui

# Configurações do Servidor
PORT=3000
NODE_ENV=development

# Frontend URL (para CORS)
# Substitua pela sua URL do Vercel
FRONTEND_URL=https://your-app-name.vercel.app

# JWT Secret (gere uma chave segura em produção)
JWT_SECRET=sua_chave_secreta_jwt_muito_longa_e_segura_aqui
